        link(rel='stylesheet', href='#{basePath}plugs/datetimepicker/bootstrap-datetimepicker.min.css')
        link(rel='stylesheet', href='#{basePath}css/main.css')
        link(rel='stylesheet', href='#{basePath}plugs/metisMenu/metisMenu.min.css')        
        link(rel='stylesheet', href='#{basePath}css/foot.css')   
        link(rel='stylesheet', href='#{basePath}css/sb-admin-2.css') 
        <!--[if lt IE 9]>
        script(src="#{basePath}plugs/html5shiv.min.js")
